<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.edu.fisi.unmsm.proyectosegsil">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_segsil"
        android:label="@string/siglas_sistema"
        android:roundIcon="@drawable/icon_segsil"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DirectorActivity" />
        <activity android:name=".SilabusRegistrarActivity" />
        <activity android:name=".AvanceRegistrarActivity" />
        <activity android:name=".ControlSilabusActivity" />
        <activity android:name=".ControlAvanceActivity" />
        <activity
            android:name=".AdminActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".SilabusMenuActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".ControlMenuActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".AvanceMenuActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".BienvenidoActivity" />
    </application>

</manifest>